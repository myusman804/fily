#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/bin/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/bin/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/bin/cli" "$@"
else
  exec node  "$basedir/../.pnpm/expo@53.0.20_@babel+core@7.28.3_@expo+metro-runtime@5.0.4_react-native@0.79.5_@babel+co_9e4972109b227495eb285bbc28e9a950/node_modules/expo/bin/cli" "$@"
fi

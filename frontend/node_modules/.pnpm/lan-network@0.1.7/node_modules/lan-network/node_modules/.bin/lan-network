#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules/lan-network/dist/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules/lan-network/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules/lan-network/dist/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules/lan-network/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/lan-network@0.1.7/node_modules:/home/fullyreactnative/master-soon/new/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/lan-network-cli.js" "$@"
else
  exec node  "$basedir/../../dist/lan-network-cli.js" "$@"
fi
